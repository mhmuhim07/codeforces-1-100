#include <bits/stdc++.h>
 
using namespace std;
 
int main()
{
    int t;
    cin>>t;
    while(t--){
        string s;
        int n;
        cin>>n;
        cin>>s;
        //sort(s.begin(),s.end());
        int c = 0;
        for(int i=0;i<s.size();i++){
            if(s[i]=='Q')c++;
            else{
                c=c-1;
                if(c<0)c=0;
            }
        }if(c==0)
        cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
        
    }
 
    return 0;
}
