#include<iostream>
using namespace std;
int main()
{
long long n,m,a,n1,m1;
cin>>n>>m>>a;
n1=n%a;
m1=m%a;
if(n1>0&&m1==0){
    cout<<(m/a)*((n/a)+1)<<endl;
}
else if(n1==0&&m1>0){
    cout<<(n/a)*((m/a)+1)<<endl;
}
else if(n1>0&&m1>0){
    cout<<((m/a)+1)*((n/a)+1)<<endl;}
else if(n1==0&&m1==0){
    cout<<(n/a)*(m/a)<<endl;
}
return 0;
}
