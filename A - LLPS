
#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).rbegin(),(v).rend()
#define test int t; cin>>t; for(int no = 1;no<=t;no++)
#define pb push_back
#define endl '\n'
#define ff first
#define ss second
#define fast ios_base::sync_with_stdio(false);cin.tie(0),cout.tie(0);
void solve()
{
    string s;
    cin>>s;
    sort(all(s));
    char a = s[0];
    fo(i,0,s.size()) {
        if(s[i]==a) cout<<a;
        else break;

    }
    cout<<endl;

}
int main()
{

    fast;

    int t=1;
    //cin >> t;
    while(t--)
    {
        solve();
    }

    return 0;
}
