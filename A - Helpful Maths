#include <bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cin>>s;
    int n=s.size()/2;
    sort(s.begin(),s.end());
    int sz=s.size()-1;
    for(int i=0;i<s.size()-1;i++) {
    if(s[i]!='+')cout<<s[i]<<'+';
   // else
    //cout<<s[i]<<'+';
    }
    
    cout<<s[sz]<<endl;
    return 0;
}
  
