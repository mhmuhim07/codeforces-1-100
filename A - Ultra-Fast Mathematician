#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);
    string v1;
    string v2;
    cin>>v1>>v2;
    //reverse(v1.begin(),v1.end());
    //reverse(v2.begin(),v2.end());
    for(int i=0;i<v1.size();i++){
        if(v1[i]=='1'&&v2[i]=='0') cout<<"1";
        else if(v1[i]=='0'&&v2[i]=='1') cout<<"1";
        else if(v1[i]=='1'&&v2[i]=='1') cout<<"0";
        else if(v1[i]=='0'&&v2[i]=='0') cout<<"0";
    }
    cout<<endl;

  
    return 0;
}
