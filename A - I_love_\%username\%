#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);

    int t;
    cin >> t;
    int a;
    cin>>a;
    int count =0,max,min;
    max=a;
    min=a;
    fo(i,1,t)
    {
       cin>>a;
       if(max<a){
        count++;
        max=a;
       }
       else if(min>a){
        count++;
        min=a;
       }

    }
    cout<<count<<endl;
  
    return 0;
}
