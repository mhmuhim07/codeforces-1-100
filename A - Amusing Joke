#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);

    int t=1;
    //cin >> t;
    while(t--){
        string s,s1,s2;
        cin>>s>>s1>>s2;
        string m = s+s1;
        sort(m.begin(),m.end());
        sort(s2.begin(),s2.end());
        if(m==s2) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }

    return 0;
}
