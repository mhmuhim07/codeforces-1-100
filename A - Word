#include<bits/stdc++.h>
using namespace std;
int main()
{

string s;
cin>>s;
int i,c=0;
for(i=0;i<s.size();i++){
    if(s[i]>=65&&s[i]<=90)
        c++;
}
if(c>s.size()-c){
     for (i=0;i<s.size();i++){
        if(s[i]>=97&&s[i]<=122){
            s[i]=s[i]-32;
                 cout<<s[i];}
                 else
                    cout<<s[i];

     }

}
else
for (i=0;i<s.size();i++){
     if(s[i]>=65&&s[i]<=90){
         s[i]=s[i]+32;
                 cout<<s[i];
     }
     else
                    cout<<s[i];
}

}
