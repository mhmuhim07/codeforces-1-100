#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);

    int n;
    cin>>n;
    if(n%2==1) cout<<"-1"<<endl;
    else{
        for(int i=n;i>=1;i--) cout<<i<<" ";
    }
cout<<endl;


    return 0;
}
