#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()
#define test int t; cin>>t; for(int no = 1;no<=t;no++)
#define pb push_back
#define endl '\n'
#define ff first
#define ss second
#define fast ios_base::sync_with_stdio(false);cin.tie(0),cout.tie(0);
void solve()
{
int n,a,b;
cin>>n>>a>>b;
if((a+b)>=n){
    if(n==a&&n==b) cout<<"Yes"<<endl;
    else cout<<"No"<<endl;
}
else{
    if(n-(a+b)>1) cout<<"Yes"<<endl;
    else cout<<"No"<<endl;
}
}
int main()
{

    fast;

    int t=1;
    cin >> t;
    while(t--)
    {
        solve();
    }

    return 0;
}
