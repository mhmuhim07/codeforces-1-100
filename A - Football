#include <bits/stdc++.h>
using namespace std;

int main() {
    string s;
    cin>>s;
    int ans=1;
    for(int i=0;i<s.size();i++){
        if(s[i]==s[i+1]){
        ans++;
        if(ans==7){
            cout<<"YES"<<endl;
            return 0;
        }
    }
     else
            ans=1;

    }
     if(ans<7) cout<<"NO"<<endl;
    return 0;
}
