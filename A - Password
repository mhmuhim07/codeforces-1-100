#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        int sum=10-n;
        sum=sum-1;
        int s=(sum*(sum+1))/2;


        cout<<(s*6)<<endl;
    }


    return 0;
}
