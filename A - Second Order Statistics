#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);
    int n;
    cin>>n;
    set<int>s;
    fo(i,0,n){
        int a;
        cin>>a;
        s.insert(a);
    }
    if(s.size()==1) cout<<"NO"<<endl;
    else{
        cout<<*(++s.begin())<<endl;
    }
  
    return 0;
}
