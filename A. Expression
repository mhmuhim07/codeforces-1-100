#include<bits/stdc++.h>
using namespace std;
#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()
#define test int t; cin>>t; for(int no = 1;no<=t;no++)
#define pb push_back
#define endl '\n'
#define ff first
#define ss second
#define fast ios_base::sync_with_stdio(false);cin.tie(0),cout.tie(0);
void solve()
{
    int a,b,c;
    cin>>a>>b>>c;
    vector<int>v;
    int ans = a+b*c;
    int ans1 =a*(b+c) ;
    int ans2 =a*b*c ;
    int ans3 = (a+b)*c;
    int ans4=a+b+c;
    v.pb(ans);
    v.pb(ans1);
    v.pb(ans2);
    v.pb(ans3);
    v.pb(ans4);
    sort(all(v));
    cout<<v[4]<<endl;

}
int main()
{

    fast;

    int t=1;
    //cin >> t;
    while(t--)
    {
        solve();
    }

    return 0;
}

