#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define fo(i,a,n) for(ll i=a;i<n;i++)
#define all(v) (v).begin(),(v).end()

#define pb push_back
#define endl '\n'

int main() {

    ios_base::sync_with_stdio(false);
    cin.tie(0),cout.tie(0);
    int t; 
    cin>>t;
    map<string,int>m;
    while(t--){

    string s;
    cin>>s;
    if(m[s]==0) cout<<"OK"<<endl;
    else cout<<s<<m[s]<<endl;
    m[s]++;



    }

    return 0;
}
