#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin>>n;
    int sum=0,sum1=0,sum2=0;
   for(int i=0;i<n;i++){
    int x,y,z;
    cin>>x>>y>>z;
    sum+=x;
    sum1+=y;
    sum2+=z;
   }


   if(sum==0&&sum1==0&&sum2==0) cout<<"YES"<<endl;
   else cout<<"NO"<<endl;
    return 0;
}
